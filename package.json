{
  "name": "kanahei-wallpapers",
  "version": "0.1.0",
  "description": "A utility to download all kanahei wallpapers",
  "bin": {
    "kanahei": "dist/cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "ext:watch": "webpack --watch",
    "ext:build": "cross-env NODE_ENV=production webpack && npm run archive",
    "archive": "node ./bin/archive",
    "prepublish": "npm run build"
  },
  "author": "Yukai Huang",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "cross-fetch": "^2.2.1",
    "got": "^8.3.1",
    "tempfile": "^2.0.0",
    "wallpaper": "^3.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "archiver": "^2.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
