{
  "name": "kanahei-wallpapers",
  "version": "0.5.0",
  "description": "A utility to download all kanahei wallpapers",
  "bin": {
    "kanahei": "dist/cli.js"
  },
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "update-kanahei": "node ./bin/fetchWallpapers",
    "watch": "tsc -w",
    "build": "tsc",
    "ext:watch": "webpack --watch",
    "ext:build": "cross-env NODE_ENV=production webpack",
    "ext:archive": "node ./bin/archive",
    "ext:prepare": "npm run ext:build && npm run ext:archive",
    "ext:release": "npm run ext:build && cd extension_dist && webstore upload --auto-publish",
    "prepublish": "npm run build",
    "test": "mocha -r ts-node/register test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukaii/kanahei-wallpapers.git"
  },
  "keywords": [
    "kanahei",
    "wallpaper-cli",
    "fun"
  ],
  "author": {
    "name": "Yukai Huang",
    "email": "yukaihuangtw@gmail.com",
    "url": "https://yukaii.tw"
  },
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@xunlei/vue-context-menu": "^1.0.2",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-fetch": "^2.2.3",
    "got": "^8.3.2",
    "localforage": "^1.7.3",
    "p-iteration": "^1.1.7",
    "primer": "^10.10.5",
    "tempfile": "^2.0.0",
    "vue": "^2.6.6",
    "vue-class-component": "^6.3.2",
    "vue-i18n": "^8.8.1",
    "vue-property-decorator": "^7.3.0",
    "wallpaper": "^3.0.0",
    "webextension-polyfill": "^0.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cheerio": "^0.22.10",
    "@types/firefox-webext-browser": "^58.0.4",
    "@types/mocha": "^5.2.5",
    "archiver": "^2.1.1",
    "chai": "^4.2.0",
    "chrome-webstore-upload-cli": "^1.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dotenv": "^6.2.0",
    "glob-all": "^3.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "purgecss-webpack-plugin": "^1.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2",
    "vue-loader": "^15.6.2",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
}
